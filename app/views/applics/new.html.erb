<h1>New Application </h1>

<%= form_for(@applic) do |f| %>
  <div class ="field">
    <%= f.label      :name %> </br>
    <%= f.text_field :name %> </br>
  </div>
  <div class ="field">
    <%= f.label      :status %> </br>
    <%= f.text_field :status %> </br>
  </div>
  <div class ="field">
    <%= f.label      :version %> </br>
    <%= f.text_field :version %> </br>
  </div>
  <div class ="field">
    <%= f.label      :description %> </br>
    <%= f.text_field :description %> </br>
  </div>
  <div class ="field">
    <%= f.label      :MaintainenceDay %> </br>
    <%= f.text_field :maintwin_day %> </br>
  </div>
  <div class ="field">
    <%= f.label      :start_time %> </br>
    <%= f.text_field :start_time %> </br>
  </div>
  <div class ="field">
    <%= f.label      :end_time %> </br>
    <%= f.text_field :end_time %> </br>
  </div>
  <div class ="actions">
    <%= f.submit     "Create New Admin" %> </br>
  </div>


<% end %>


